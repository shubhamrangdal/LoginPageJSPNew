<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" id="WebApp_ID" version="3.0">
  <display-name>LoginPageJSP</display-name>
  <welcome-file-list>
    <welcome-file>Login.jsp</welcome-file>
  </welcome-file-list>
  <filter>
    <filter-name>LoginFilter</filter-name>
    <filter-class>com.bitwise.Login.container.LoginFilter</filter-class>
  </filter>
  <filter-mapping>
    <filter-name>LoginFilter</filter-name>
    <url-pattern>/LoginPage</url-pattern>
  </filter-mapping>
  <servlet>
    <servlet-name>LoginPage</servlet-name>
    <servlet-class>com.bitwise.Login.container.LoginPage</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>LoginPage</servlet-name>
    <url-pattern>/LoginPage</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>EducationValidate</servlet-name>
    <servlet-class>com.bitwise.Login.container.EducationValidate</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>EducationValidate</servlet-name>
    <url-pattern>/EducationValidate</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>OccupationValidate</servlet-name>
    <servlet-class>com.bitwise.Login.container.OccupationValidate</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>OccupationValidate</servlet-name>
    <url-pattern>/OccupationValidate</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>ResidentialValidate</servlet-name>
    <servlet-class>com.bitwise.Login.container.ResidentialValidate</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>ResidentialValidate</servlet-name>
    <url-pattern>/ResidentialValidate</url-pattern>
  </servlet-mapping>
</web-app>